#ifndef CPP_2024_POINT_H
#define CPP_2024_POINT_H
using namespace std;
#include <algorithm>
#include <vector>

class Point {
private:
    int x, y;
public:
    Point( int x=0, int y=0);
    int getX() const;
    int getY() const;
    double distanceTo(const Point& point) const;
};


#endif //CPP_2024_POINT_H
